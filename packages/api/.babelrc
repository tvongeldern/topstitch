{
	"presets": [
		"@babel/preset-env"
	],
	"ignore": ["/node_modules"],
	"plugins": [
		[
			"module-resolver",
			{
				"root": "./",
				"alias": {
					"@constants": "./src/constants",
					"@db": "./src/db",
					"@middleware": "./src/middleware",
					"@models": "./src/models",
					"@routes": "./src/routes",
					"@utils": "./src/utils"
				}
			}
		],
		"@babel/plugin-transform-runtime",
		"@babel/plugin-proposal-class-properties"
	]
}