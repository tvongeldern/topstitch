{
	"presets": [
		"@babel/preset-env"
	],
	"ignore": ["/node_modules"],
	"plugins": [
		[
			"module-resolver",
			{
				"root": "./",
				"alias": {
					"@config": "./src/config",
					"@db": "./src/db",
					"@middleware": "./src/middleware",
					"@models": "./src/models",
					"@routes": "./src/routes",
					"@utils": "./src/utils"
				}
			}
		]
	]
}