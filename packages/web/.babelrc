{
	"presets": [
		"next/babel"
	],
	"plugins": [
		[
			"module-resolver",
			{
				"root": "./",
				"alias": {
					"@assets": "./assets",
					"@components": "./src/components",
					"@constants": "./src/constants",
					"@forms": "./src/forms",
					"@garment-builders": "./src/garment-builders",
					"@icons": "./src/icons",
					"@state": "./src/state",
					"@utils": "./src/utils"
				}
			}
		],
		"inline-react-svg"
	]
}
